<report xmlns="http://jfreereport.sourceforge.net/namespaces/reports/legacy/simple" name="Cierre de Caja" pageformat="A4"
       topmargin="25" leftmargin="25" bottommargin="10" rightmargin="10">
	
        <reportheader fontname="SanSerif" fontstyle="plain" fontsize="08">
        
            <!-- INICIO HEADER 1 -->
	    
	    <line x1="0" y1="80"  x2="550" y2="80"  weight=".75" color="#AAAAAA"/>
            <line x1="370" y1="0" x2="370" y2="80"  weight=".75" color="#AAAAAA"/>
            
            <image-field name="logo" x="0" y="5" width="52" height="30"  scale="true" fieldname="logo"/>
            <string-field        x="70"  y="00" width="300" height="28" dynamic="false" alignment="center" vertical-aligment="middle" fontsize="14" fsbold="true" fieldname="orgtitle"/>
            <rectangle name="ta" x="70"  y="30" width="300" height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field           x="70" y="30" width="300" height="10" dynamic="false" alignment="center" fontsize="08" fsbold="true" vertical-aligment="middle" fieldname="locationdata"/>
            <rectangle name="ta" x="0"   y="52" width="230" height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="0"   y="52" width="230" height="10" dynamic="false" alignment="left" fontsize="08" fieldname="address"/>
            
            <rectangle name="ta" x="240" y="52" width="45"  height="10" draw="false" fill="false" color="#0000FF"/>
            <label     name="ti" x="240" y="52" width="45"  height="10" dynamic="false" alignment="left" fontsize="08" fsbold="false">Teléfonos:</label>
            <rectangle name="ta" x="290" y="52" width="75" height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="290" y="52" width="75" height="10" dynamic="false" alignment="left" fontsize="08" fieldname="phonenumber1"/>
            <rectangle name="ta" x="290" y="65" width="75" height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="290" y="65" width="75" height="10" dynamic="false" alignment="left" fontsize="08" fieldname="phonenumber2"/>
            <rectangle name="ta" x="0"   y="65" width="190" height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="0"   y="65" width="190" height="10" dynamic="false" alignment="left" fontsize="08" fieldname="webaddress"/>
            

            <label     name="zo" x="380" y="00"  width="200" height="10" fontsize="08" dynamic="false" alignment="center" vertical-aligment="middle" fsbold="true">CIERRE DE CAJA</label>
	    <string-field        x="380" y="10"  width="200" height="08" fontsize="05" dynamic="false" alignment="center" fieldname="caja"/>
            
            <rectangle name="ta" x="380" y="30"  width="240" height="12" draw="false" fill="false" color="#0000FF"/>
            <rectangle name="ta" x="380" y="52"  width="60"  height="10" draw="false" fill="false" color="#0000FF"/>
            <label     name="zo" x="380" y="52"  width="60"  height="10" fontsize="08" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">C.U.I.T.:</label>
            <rectangle name="ta" x="470" y="52"  width="200" height="10" draw="false" fill="false" color="#0000FF"/>
	    <string-field        x="470" y="52"  width="200" height="10" alignment="left" dynamic="false" fontsize="08" fsbold="false" fieldname="cuit"/>
            <rectangle name="ta" x="380" y="55"  width="60"  height="10" draw="false" fill="false" color="#0000FF"/>
            <label     name="zo" x="380" y="65"  width="60"  height="10" fontsize="08" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">Fecha Impr.:</label>
            <rectangle name="ta" x="470" y="65"  width="200" height="10" draw="false" fill="false" color="#0000FF"/>
            <string-field          x="470" y="65"  width="200" height="10" fontsize="08" dynamic="true"  alignment="left" fieldname="actualdate"/>
            
            <!-- FIN HEADER 1 -->
            
            <!-- INICIO HEADER 2 -->
            
            <rectangle  name="ta" x="0"   y="90"  width="60"  height="12" draw="false" fill="false" color="#0000FF"/>
            <label      name="zo" x="0"   y="90"  width="60"  height="12" fontsize="10" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">Cajero:</label>
            <rectangle  name="ta" x="65"  y="90"  width="300" height="12" draw="false" fill="false" color="#0000FF"/>
            <string-field x="65"  y="90"  width="300" height="12" alignment="left" dynamic="false" fontsize="10" fsbold="true" fieldname="cajero"/>

            <label      name="zo" x="0"   y="110"  width="100"  height="12" fontsize="10" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">Fecha de Apertura:</label>
            <string-field x="105"  y="110"  width="70" height="12" alignment="left" dynamic="false" fontsize="10" fsbold="true" fieldname="fechaapertura"/>
            <label      name="zo" x="300"   y="110"  width="100"  height="12" fontsize="10" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">Fecha de Cierre:</label>
            <string-field x="405"  y="110"  width="70" height="12" alignment="left" dynamic="false" fontsize="10" fsbold="true" fieldname="fechacierre"/>
            <label      name="zo" x="0"   y="125"  width="100"  height="12" fontsize="10" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">Hora de Apertura:</label>
            <string-field x="105"  y="125"  width="70" height="12" alignment="left" dynamic="false" fontsize="10" fsbold="true" fieldname="horaapertura"/>
            <label      name="zo" x="300"   y="125"  width="100"  height="12" fontsize="10" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">Hora de Cierre:</label>
            <string-field x="405"  y="125"  width="70" height="12" alignment="left" dynamic="false" fontsize="10" fsbold="true" fieldname="horacierre"/>
            <label      name="zo" x="0"   y="140"  width="100"  height="12" fontsize="10" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">Monto de Apertura:</label>
            <number-field x="105"  y="140"  width="70" height="12" alignment="left" dynamic="false" fontsize="10" fsbold="true" format="$ #,##0.00" fieldname="montoapertura"/>
            <label      name="zo" x="300"   y="140"  width="100"  height="12" fontsize="10" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">Recaudación:</label>
            <number-field x="405"  y="140"  width="70" height="12" alignment="left" dynamic="false" fontsize="10" fsbold="true" format="$ #,##0.00" fieldname="recaudacion"/>
            <label      name="zo" x="0"   y="155"  width="100"  height="12" fontsize="10" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">Monto de Cierre:</label>
            <number-field x="105"  y="155"  width="70" height="12" alignment="left" dynamic="false" fontsize="10" fsbold="true" format="$ #,##0.00" fieldname="montocierre"/>
           <label      name="zo" x="300"   y="155"  width="100"  height="12" fontsize="10" dynamic="false" alignment="left" vertical-aligment="middle" fsbold="false">Diferencia:</label>
            <number-field x="405"  y="155"  width="70" height="12" alignment="left" dynamic="false" fontsize="10" fsbold="true" format="$ #,##0.00" fieldname="diferencia"/>
	</reportheader>
