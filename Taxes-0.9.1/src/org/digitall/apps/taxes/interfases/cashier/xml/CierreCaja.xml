         <groups>
                <group name="Header-Footer">

                        <groupheader repeat="true" layout-cachable="false" pagebreak-before-print="false" fontname="SanSerif" fontstyle="plain">
                                <rectangle name="tangle"  x="0"     y="10" width="100"  height="18"  draw="true" fill="false" color="#000000"/>
                                <rectangle name="tangle"  x="100"   y="10" width="100"  height="18"  draw="true" fill="false" color="#000000"/>
                                <rectangle name="tangle"  x="200"   y="10" width="140"  height="18"  draw="true" fill="false" color="#000000"/>
                                <rectangle name="tangle"  x="340"   y="10" width="100" height="18"  draw="true" fill="false" color="#000000"/>
                                <rectangle name="tangle"  x="440"   y="10" width="110"  height="18"  draw="true" fill="false" color="#000000"/>
                               
                                <label name="fecha"  x="0"   y="10" width="100"  height="18" dynamic="true" alignment="center" vertical-alignment="middle" fontsize="09">Fecha</label>
                                <label name="hora"   x="100" y="10" width="100"  height="18" dynamic="true" alignment="center" vertical-alignment="middle" fontsize="09">Hora</label>
                                <label name="tipo"   x="200" y="10" width="140"  height="18" dynamic="true" alignment="center" vertical-alignment="middle" fontsize="09">Tipo</label>
                                <label name="ticket" x="340" y="10" width="100"  height="18" dynamic="true" alignment="center" vertical-alignment="middle" fontsize="09"># Ticket</label>
                                <label name="monto"  x="440" y="10" width="110"  height="18" dynamic="true" alignment="center" vertical-alignment="middle" fontsize="09">Monto</label>
                        </groupheader>

			<groupfooter height="18" fontname="Monospaced" fontstyle="bold" fontsize="9">
                                <line x1="0" y1="0" x2="550" y2="0" weight="0.5"/>
                            	<label name="l" x="360" y="7"  width="55"  fontsize="10" dynamic="true" alignment="right" fontname="SansSerif" fsbold="true" >Total:</label>
				<rectangle      x="430" y="5"  width="120" height="15" color="#F6F6F6" draw="true" fill="true"/>
				<rectangle      x="430" y="5"  width="120" height="15" color="#000000" draw="true" fill="false"/>
				<number-field   x="430" y="7"  width="110" dynamic="true" alignment="right" fontname="SansSerif" fontsize="9" fsbold="true" format="$ #,##0.00" fieldname="totalamount" />
				
				<line x1="0"   y1="100" x2="150" y2="100" weight="0.25"/>
				<line x1="200" y1="100" x2="350" y2="100" weight="0.25"/>
				<line x1="390" y1="100" x2="540" y2="100" weight="0.25"/>
				
				<label x="0"   y="100" name="lbltitle" dynamic="true" width="150" alignment="center">Firma</label>
				<label x="200" y="100" name="lbltitle" dynamic="true" width="150" alignment="center">Aclaración</label>
				<label x="390" y="100" name="lbltitle" dynamic="true" width="150" alignment="center">DNI</label>
      			</groupfooter>
		</group>

	</groups>

        <items layout-cachable="false" >
            <band name="table_0" fontsize="08">
                    
                    <line x1="0"   y1="0" x2="0"   y2="100%" weight="0.25"/>
                    <line x1="100" y1="0" x2="100" y2="100%" weight="0.25"/>
                    <line x1="200" y1="0" x2="200" y2="100%" weight="0.25"/>
                    <line x1="340" y1="0" x2="340" y2="100%" weight="0.25"/>
                    <line x1="440" y1="0" x2="440" y2="100%" weight="0.25"/>
                    <line x1="550" y1="0" x2="550" y2="100%" weight="0.25"/>

                    <date-field   name="fecha"  x="0"   y="2"  width="100"  height="10" alignment="center" vertical-alignment="middle" dynamic="false" format="dd/MM/yyyy" fieldname="table_0.fecha"/>
                    <date-field   name="hora"   x="100" y="2"  width="100"  height="10" alignment="center" vertical-alignment="middle" dynamic="false" format="HH:mm:ss" fieldname="table_0.hora"/>
                    <string-field name="tipo"   x="200" y="2"  width="140"  height="10" alignment="center" vertical-alignment="middle" dynamic="false" fieldname="table_0.nombre"/>
                    <string-field name="numero" x="340" y="2"  width="100"  height="10" alignment="center" vertical-alignment="middle" dynamic="false" fieldname="table_0.numero"/>
                    <number-field name="monto"  x="440" y="2"  width="100"  height="10" alignment="right"  vertical-alignment="middle" dynamic="false" format="$ #,##0.00" fieldname="table_0.monto1"/>
            </band>
	</items>

        <pagefooter height="24">
		<line x1="0" y1="0" x2="550" y2="0" weight="0.5"/>
                <string-field fieldname="location" x="0" y="13" width="100%" height="10" alignment="left" vertical-alignment="top"/>
		<string-field fieldname="pageXofY" x="0" y="13" width="550"  height="10" alignment="right" vertical-alignment="top"/>
	</pagefooter>

	<functions>
		<function class="org.jfree.report.function.HideElementByNameFunction" name="hideHeader">
			<properties>
				<property name="element">table_0</property>
				<property name="field">TablePrefix</property>
			</properties>
		</function>
		
		<function class="org.jfree.report.function.HideElementByNameFunction" name="hideDetail">
			<properties>
				<property name="element">table_0</property>
				<property name="field">TablePrefix</property>
			</properties>
		</function>
		

                <function class="org.jfree.report.function.ItemSumFunction" name="totalamount">
			<properties>
				<property name="field">table_0.monto1</property>
				<property name="group">Header-Footer</property>
			</properties>
		</function>
                <function class="org.jfree.report.function.PageOfPagesFunction" name="pageXofY">
                        <properties>
                                <property name="format">Página {0} de {1}</property>
                        </properties>
                </function>
	</functions>
</report>


